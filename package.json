  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hfreire/health-checkup.git"
  },
  "author": "Hugo Freire <hugo@exec.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hfreire/health-checkup/issues"
  },
  "homepage": "https://github.com/hfreire/health-checkup#readme",
  "dependencies": {
    "blue-tape": "^1.0.0",
    "bluebird": "^3.4.6",
    "locks": "^0.2.2",
    "memoizee": "^0.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.14.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.0",
    "istanbul": "^0.4.5",
    "standard": "^8.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint .",
    "coverage": "node_modules/.bin/istanbul cover --report lcovonly test/*",
