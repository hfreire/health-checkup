{
  "name": "health-checkup",
  "description": "A NodeJS health check facility to check the status of your modules",
  "version": "0.0.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hfreire/health-checkup.git"
  },
  "author": "Hugo Freire <hugo@exec.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hfreire/health-checkup/issues"
  },
  "homepage": "https://github.com/hfreire/health-checkup#readme",
  "dependencies": {
    "blue-tape": "1.0.0",
    "bluebird": "3.4.7",
    "locks": "0.2.2",
    "memoizee": "0.4.2"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "coveralls": "2.11.16",
    "eslint": "3.15.0",
    "eslint-config-standard": "7.0.0-beta.0",
    "eslint-plugin-mocha": "4.8.0",
    "eslint-plugin-node": "4.0.1",
    "eslint-plugin-promise": "3.4.2",
    "eslint-plugin-standard": "2.0.1",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "semantic-release": "6.3.2",
    "pre-git": "3.13.0",
    "standard": "9.0.0-beta.0",
    "testdouble": "1.11.1"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  },
  "scripts": {
    "clean": "rm -rf lib coverage",
    "lint": "./node_modules/.bin/eslint .",
    "coverage": "node_modules/.bin/istanbul cover --include-all-sources --root src --print detail ./node_modules/mocha/bin/_mocha -- --recursive test",
    "test": "npm run lint && npm run coverage",
    "compile": "mkdir -p lib && ./node_modules/.bin/babel -o lib/index.js src/health-checkup.js",
    "prepublish": "npm run compile",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "semantic-release": "./node_modules/.bin/semantic-release pre && npm publish && node_modules/.bin/semantic-release post",
    "commit": "./node_modules/.bin/commit-wizard"
  }
}
